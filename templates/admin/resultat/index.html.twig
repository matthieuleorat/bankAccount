{% block main %}
    {{ htmlTree|raw }}
    {{ form_start(form) }}
        {{ form_rest(form) }}
    {{ form_end(form) }}
    <div id="tester"></div>
    <script>
        var trace1 = {
            x: ['giraffes', 'orangutans', 'monkeys'],
            y: [20, 14, 23],
            name: 'SF Zoo',
            type: 'bar'
        };

        var trace2 = {
            x: ['giraffes', 'orangutans', 'monkeys'],
            y: [12, 18, 29],
            name: 'LA Zoo',
            type: 'bar'
        };

        var data = [trace1, trace2];

        var layout = {barmode: 'group'};

        Plotly.newPlot('tester', data, layout);
    </script>
{% endblock %}